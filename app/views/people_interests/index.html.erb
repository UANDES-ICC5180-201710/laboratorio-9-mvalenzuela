<p id="notice"><%= notice %></p>

<h1>People Interests</h1>

<table>
  <thead>
    <tr>
      <th>Person</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @people_interests.each do |people_interest| %>
      <tr>
        <td><%= people_interest.person %></td>
        <td><%= people_interest.interest_course %></td>
        <td><%= link_to 'Show', people_interest %></td>
        <td><%= link_to 'Edit', edit_people_interest_path(people_interest) %></td>
        <td><%= link_to 'Destroy', people_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New People Interest', new_people_interest_path %>
